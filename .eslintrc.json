{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "comma-dangle": ["error", "never"],
        "no-unused-vars": ["warn"],
        "linebreak-style": "off",
        "import/no-extraneous-dependencies": "off"
           
    }
}
